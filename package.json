{
  "name": "ecommerce-application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn prepare && vite",
    "build": "tsc && yarn lint && yarn stylelint && yarn test:staged && vite build",
    "preview": "vite preview",
    "lint": "node -e \"console.log('\\x1b[36m%s\\x1b[0m', '[INFO]:', 'Lint script started');\" && eslint \"**/*.{js,jsx,ts,tsx,cjs,mjs}\" --ignore-path .gitignore",
    "lint:write": "node -e \"console.log('\\x1b[36m%s\\x1b[0m', '[INFO]:', 'Lint script started write mode');\" && eslint \"**/*.{js,jsx,ts,tsx,cjs,mjs}\" --ignore-path .gitignore --fix",
    "stylelint": "node -e \"console.log('\\x1b[36m%s\\x1b[0m', '[INFO]:', 'Stylelint script started');\" && stylelint \"**/*.{css,scss,sass,less,styl,vue,svelte}\" --ignore-path .gitignore",
    "stylelint:write": "node -e \"console.log('\\x1b[36m%s\\x1b[0m', '[INFO]:', 'Stylelint script started write mode');\" && stylelint \"**/*.{css,scss,sass,less,styl,vue,svelte}\" --ignore-path .gitignore --fix",
    "format": "node -e \"console.log('\\x1b[36m%s\\x1b[0m', '[INFO]:', 'Format script started');\" && prettier **/* --write --ignore-path .gitignore",
    "prepare": "node -e \"console.log('\\x1b[36m%s\\x1b[0m', '[INFO]:', 'Prepare script started');\" && husky",
    "test": "node -e \"console.log('\\x1b[36m%s\\x1b[0m', '[INFO]:', 'Test script started');\" && vitest",
    "test:staged": "node -e \"console.log('\\x1b[36m%s\\x1b[0m', '[INFO]:', 'Test script started staged mode');\" && vitest --run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@commercetools/sdk-client-v2": "^2.5.0",
    "@hookform/resolvers": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.4",
    "react-router-dom": "^6.23.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/format": "^19.3.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/sort-by": "^1",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "meow": "9.0.0",
    "optionator": "0.9.1",
    "postcss": "^8.4.38",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "sass": "^1.76.0",
    "sort-by": "^1.2.0",
    "stylelint": "^16.4.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.2.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.2.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-sass-dts": "^1.3.18",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "volta": {
    "node": "20.12.2",
    "yarn": "4.1.1"
  },
  "engines": {
    "yarn": "4.1.1",
    "node": "20.12.2"
  },
  "packageManager": "yarn@4.1.1"
}
